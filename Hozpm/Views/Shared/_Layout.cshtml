<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - My ASP.NET Application</title>

	@Styles.Render("~/css/bootstrap.css", "~/css/bootstrap-theme.css", "~/css/site.css")
	@RenderSection("styles", false)
</head>
<body>

<div id="wrap">
	<header>
		@Html.Partial("Partial/Layout/Header")
	</header>

	<main class="container">
		<nav>
			@Html.Partial("Site/Breadcrumb")
		</nav>

		@RenderBody()
	</main>
</div>

<footer>
	@Html.Partial("Partial/Layout/Footer")
</footer>

@Scripts.Render("~/js/jquery.js", "~/js/bootstrap.js")
@RenderSection("scripts", false)
<script type="text/javascript" src="//yastatic.net/es5-shims/0.0.2/es5-shims.min.js" charset="utf-8"></script>
<script type="text/javascript" src="//yastatic.net/share2/share.js" charset="utf-8"></script>

</body>
</html>